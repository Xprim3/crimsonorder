{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write .",
    "lint:check": "eslint . --ext .vue,.js,.ts,.jsx,.tsx",
    "format:check": "prettier --check .",
    "analyze": "vite-bundle-analyzer",
    "lighthouse": "lighthouse http://localhost:4173 --output=html --output-path=./lighthouse-report.html",
    "performance": "npm run build && npm run preview & sleep 5 && npm run lighthouse",
    "type-check": "vue-tsc --noEmit",
    "test:build": "npm run build && npm run preview"
  },
  "dependencies": {
    "primeicons": "^7.0.0",
    "primevue": "^3.53.1",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-vue": "^9.33.0",
    "lighthouse": "^12.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-bundle-analyzer": "^0.23.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.10"
  }
}
